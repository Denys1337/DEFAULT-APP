<section class="contactus">
<div class="container">
  <img src="{{root}}images/tree-big.png">
   
    <div class="block">
      <h2>Contact Us</h2>
  <div class="form">
  
    {{#> gform-form
    showError=false
    showConfirmation=false
    }}
    {{#> gform-body showError=showError}}
   
    {{> gform-singleLineText
    showError=showError
    customClass="test-class"
    hideLabel=false
    labelText="First Name"
    required=true
    typeTextOrPassword="text"
    fieldSize="medium"
    placeholderText="Enter First Name"
    }}
    {{> gform-singleLineText
    showError=showError
    customClass="test-class"
    hideLabel=false
    labelText="Last Name"
    required=true
    typeTextOrPassword="text"
    fieldSize="medium"
    placeholderText="Enter Last Name"
    }}
    {{> gform-singleLineText
    showError=showError
    customClass="test-class"
    hideLabel=false
    labelText="Phone Number"
    required=true
    typeTextOrPassword="text"
    fieldSize="medium"
    placeholderText="Enter Phone Number"
    }}
    {{> gform-singleLineText
    showError=showError
    customClass="test-class"
    hideLabel=false
    labelText="Address"
    required=true
    typeTextOrPassword="text"
    fieldSize="medium"
    placeholderText="Enter Address"
    }}
    {{> gform-email
    showError=showError
    customClass="test-class"
    hideLabel=false
    labelText="Email Address"
    required=true
    fieldSize="medium"
    placeholderText="Enter Email Address" 
    enableConfirmation=false
    enterEmailLabel=""
    confirmEmailLabel=""
    }}
    {{> gform-paragraphText
    showError=showError
    customClass="test-class"
    hideLabel=false
    labelText="Comments or Questions"
    required=true
    fieldSize="medium"
    placeholderText="Enter Comments or Questions"
    maxChar=20
    }}
      
      


      
      {{/gform-body}}
    {{/gform-form}}
  </div>
  <a href="#" class="submit">
    Submit
  </a>
  </div>
</div>

</section>


